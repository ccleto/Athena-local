<decoder name="sophos-fw-decoder">
    <prematch>^device_name="\w+"\stimestamp="\d+-\d\d-\d\d\w\d\d:\d\d:\d\d-\d+"</prematch>
</decoder>

<decoder name="sophos-fw-fields">
    <parent>sophos-fw-decoder</parent>
    <regex>device_name="(\w+)"\stimestamp="(\d+-\d\d-\d\d\w\d\d:\d\d:\d\d-\d+)"</regex>
    <order>device_name,time_fw</order>
</decoder>

<decoder name="sophos-fw-fields">
    <parent>sophos-fw-decoder</parent>
    <regex>device_model="(\w+)"</regex>
    <order>device_model</order>
</decoder>

<decoder name="sophos-fw-fields">
    <parent>sophos-fw-decoder</parent>
    <regex>device_serial_id="(\w+)"</regex>
    <order>device_serial_id</order>
</decoder>

<decoder name="sophos-fw-fields">
    <parent>sophos-fw-decoder</parent>
    <regex>log_type="(\w+)"</regex>
    <order>log_type</order>
</decoder>

<decoder name="sophos-fw-fields">
    <parent>sophos-fw-decoder</parent>
    <regex>log_component="(\w+)"|log_component="(\.+)"</regex>
    <order>log_component</order>
</decoder>

<decoder name="sophos-fw-fields">
    <parent>sophos-fw-decoder</parent>
    <regex>log_subtype="(\w+)"</regex>
    <order>log_subtype</order>
</decoder>

<decoder name="sophos-fw-fields">
    <parent>sophos-fw-decoder</parent>
    <regex>status="(\w+)"</regex>
    <order>status</order>
</decoder>

<decoder name="sophos-fw-fields">
    <parent>sophos-fw-decoder</parent>
    <regex>severity="(\w+)"</regex>
    <order>severity</order>
</decoder>

<decoder name="sophos-fw-fields">
    <parent>sophos-fw-decoder</parent>
    <regex>user_name="(\w+)"</regex>
    <order>user_name</order>
</decoder>

<decoder name="sophos-fw-fields">
    <parent>sophos-fw-decoder</parent>
    <regex>src_ip="(\d+.\d+.\d+.\d+)"</regex>
    <order>source_IP</order>
</decoder>

<decoder name="sophos-fw-fields">
    <parent>sophos-fw-decoder</parent>
    <regex>message="(\.+)"</regex>
    <order>message</order>
</decoder>